<p id="notice"><%= notice %></p>

<h1>Admin Navbars</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_navbars.each do |admin_navbar| %>
      <tr>
        <td><%= link_to 'Show', admin_navbar %></td>
        <td><%= link_to 'Edit', edit_admin_navbar_path(admin_navbar) %></td>
        <td><%= link_to 'Destroy', admin_navbar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Navbar', new_admin_navbar_path %>
