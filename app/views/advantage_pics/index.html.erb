<p id="notice"><%= notice %></p>

<h1>Advantage Pics</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @advantage_pics.each do |advantage_pic| %>
      <tr>
        <td><%= link_to 'Show', advantage_pic %></td>
        <td><%= link_to 'Edit', edit_advantage_pic_path(advantage_pic) %></td>
        <td><%= link_to 'Destroy', advantage_pic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Advantage Pic', new_advantage_pic_path %>
