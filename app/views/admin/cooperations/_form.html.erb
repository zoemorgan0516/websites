<%= simple_form_for [:admin, @cooperation] do |f| %>
  <%= f.input :cooperation_company,  label: "合作商户"  %>
  <%= f.input :cooperation_string,  label: "图片字段"  %>
  <%= f.input :avatar, label: "图片" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
  <div class="col-sm-10 col-sm-offset-2">
      <%= f.button :submit, "提 交", class: "btn btn-primary" %>
  </div>
<% end %>
