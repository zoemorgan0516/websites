<p id="notice"><%= notice %></p>

<h1>Admin Advantage Pics</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_advantage_pics.each do |admin_advantage_pic| %>
      <tr>
        <td><%= link_to 'Show', admin_advantage_pic %></td>
        <td><%= link_to 'Edit', edit_admin_advantage_pic_path(admin_advantage_pic) %></td>
        <td><%= link_to 'Destroy', admin_advantage_pic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Advantage Pic', new_admin_advantage_pic_path %>
